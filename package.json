{
  "name": "wx-smart-file",
  "displayName": "vscode 快速生成小程序文件插件",
  "description": "vscode 自动生成小程序文件插件",
  "keywords": [
    "vscdoe",
    "plugin",
    "miniprogram",
    "wxml",
    "wxss"
  ],
  "version": "0.0.1",
  "publisher": "haodaima",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "configuration": {
      "type": "obeject",
      "title": "plugin wx-smart-file config",
      "properties": {
        "wxSmartFile.enabled": {
          "type": "boolean",
          "default": true,
          "description": "插件是否启用"
        },
        "wxSmartFile.path": {
          "type": "string",
          "default": "pages",
          "description": "默认监听的文件夹"
        },
        "wxSmartFile.templateDir": {
          "type": "string",
          "description": "模板存放的文件夹, 模板文件名必须以 template 开头，例如 template.wxss, template.wxml"
        },
        "wxSmartFile.filename": {
          "type": "string",
          "description": "自动生成文件的文件名"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "typescript-compiler": "^1.4.1-2"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "https://github.com/haoxinjieFE/vscode-extension-wxSmartFile"
}
